# SPR Frontend Environment Variables
# Configuração para desenvolvimento local

# Backend SPR Python - APIs de Commodities, Previsões, Alertas
REACT_APP_SPR_API_URL=http://localhost:8000

# WhatsApp Server Node.js - APIs de WhatsApp
REACT_APP_WHATSAPP_URL=http://localhost:3003

# API principal (compatibilidade)
REACT_APP_API_URL=http://localhost:8000

# Google Calendar (opcional)
REACT_APP_GOOGLE_CLIENT_ID=
REACT_APP_GOOGLE_API_KEY=

# Debug mode
REACT_APP_DEBUG=true

# Environment
NODE_ENV=development

# Fast Refresh
FAST_REFRESH=true